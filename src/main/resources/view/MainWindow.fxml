<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
    <stylesheets>
        <URL value="@Extensions.css" />
    </stylesheets>

    <StackPane fx:id="commandBoxPlaceholder" styleClass="pane-with-border" VBox.vgrow="NEVER">
        <padding>
            <Insets bottom="5" left="10" right="10" top="5" />
        </padding>
    </StackPane>

    <StackPane maxHeight="100.0" minHeight="100.0" prefHeight="100.0">
        <children>
            <StackPane fx:id="resultDisplayPlaceholder" maxHeight="100" minHeight="100" prefHeight="100" styleClass="pane-with-border">
                <padding>
                    <Insets bottom="5" left="10" right="10" top="5" />
                </padding>
            </StackPane>
            <StackPane fx:id="commandBoxHelperPlaceholder" maxWidth="300.0" prefHeight="150.0" prefWidth="300.0" translateX="310.0" StackPane.alignment="CENTER_RIGHT">
                <padding>
                    <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
                </padding></StackPane>
        </children>
    </StackPane>

          <SplitPane id="splitPane" fx:id="splitPane" dividerPositions="0.4, 0.5">
        <VBox fx:id="personList" maxWidth="330.0" minWidth="330.0" prefWidth="150.0" SplitPane.resizableWithParent="false">
            <padding>
                <Insets bottom="10" left="10" right="10" top="10" />
            </padding>
            <StackPane fx:id="personListPanelPlaceholder" VBox.vgrow="ALWAYS" />
        </VBox>

              <StackPane fx:id="browserPlaceholder" prefWidth="400.0">
            <padding>
                <Insets bottom="10" left="10" right="10" top="10" />
            </padding>
        </StackPane>
      <SplitPane dividerPositions="0.5" maxWidth="150.0" minWidth="150.0" orientation="VERTICAL" prefWidth="150.0">
         <items>
            <VBox maxWidth="150.0" minWidth="150.0" prefWidth="150.0">
               <children>
                  <StackPane fx:id="browserSelectorPlaceholder" maxWidth="150.0" minWidth="150.0" prefWidth="150.0" />
               </children>
            </VBox>
            <VBox prefHeight="50.0" prefWidth="200.0">
               <children>
                  <StackPane fx:id="themeSelectorPlaceholder" prefHeight="150.0" prefWidth="200.0" />
               </children>
            </VBox>
         </items>
      </SplitPane>
    </SplitPane>
   <StackPane fx:id="wzpane" maxWidth="150.0" minWidth="150.0" prefWidth="150.0" />

    <StackPane fx:id="statusbarPlaceholder" VBox.vgrow="NEVER" />
</VBox>
